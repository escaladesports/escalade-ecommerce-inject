{
  "name": "escalade-ecommerce-inject",
  "version": "0.1.0",
  "main": "dist/index.js",
  "browser": "dist-browser/v1.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/escalade-ecommerce-inject.git"
  },
  "license": "MIT",
  "scripts": {
    "analyze": "esca-scripts analyze --input './src/inject.js' --browser",
    "build": "run-p build:main build:browser",
    "build:main": "esca-scripts build --input './src/index.js' --output './dist/index.js' --component --minify false",
    "build:browser": "esca-scripts build --input './src/index.js' --output './dist-browser/v1.js' --component --name EscaEcomm --minify false",
    "dev": "esca-scripts dev ---input './dev/dev.js, ./dev/index.html' --browser --react",
    "prepublishOnly": "npm run build"
  },
  "pre-commit": [],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.1",
    "esca-scripts": "^0.9.6",
    "mocha": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "static-server": "escaladesports/static-server",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1"
  },
  "dependencies": {
    "fetch": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0"
  }
}
